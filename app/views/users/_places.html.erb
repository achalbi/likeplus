 <% if user.places.count > 0 %>
 <div class="list-group">
    <% user.places.each do |loc| %>
        <a href="#" class="list-group-item">
            <% unless loc.name.nil? %>
            <b><%= loc.name unless loc.name.nil? %> </b>
            </br>
            <% end %>
            <%= loc.address %>
        </a>
    <% end %>
 </div>
 <% else %>
 	No places added, please add to find people around it
 <% end %>
